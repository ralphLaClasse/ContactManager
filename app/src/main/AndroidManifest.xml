<?xml version="1.0" encoding="utf-8"?>
<manifest package="lpirm.bdmob.contactmanager"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.ContactsMainActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="lpirm.bdmob.contactmanager.activity.AddNewContactActivity" >
        </activity>
        <provider
            android:name="lpirm.bdmob.contactmanager.provider.PersonalContactProvider"
            android:authorities="lpirm.bdmob.contactmanager.provider">
        </provider>

    </application>

</manifest>